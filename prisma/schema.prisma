generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Shelter {
  id          String   @id @default(cuid())
  name        String
  description String
  province    String
  region      String
  country     String
  beds        Int
  url         String
  photo       String
  latitude    Float
  longitude   Float
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Version {
  id        Int      @id @default(autoincrement())
  userid    Int
  shelteris Int
  createdAt DateTime @default(now())
}
